      <Statement>
        <ExprLine>
          <DescStr></DescStr>
          <OrigStr>
			# Set text variable
			DeclareType("RowData", "PlayerDifficulty");
			GenSetString(Self, "Text", Translate(RowData.Name));
			GenSetValue(Self, "Difficulty", RowData.Index);

			# Instance
			if (pd_InstanceDifficultyApplied())
				if (Self.State.Row = pd_GetInstanceDifficultyRow())
					GenSetValue(Self, "Disabled", 0);
					GenSetValue(Self, "Selected", 1);
				else
					GenSetValue(Self, "Disabled", 1);
				endif
			# Team
			elif (Self.Parent.Parent.Var[Team].Int)
				if (Self.State.Row = pd_GetTeamDifficultyRow())
					GenSetValue(Self, "Disabled", 0);
					GenSetValue(Self, "Selected", 1);
				elif (not team_IsTeamLeader(Player))
					GenSetValue(Self, "Disabled", 1);
				endif
			# Player
			else
				if (Self.State.Row = pd_GetPlayerDifficultyRow())
					GenSetValue(Self, "Selected", 1);
				endif
			endif
