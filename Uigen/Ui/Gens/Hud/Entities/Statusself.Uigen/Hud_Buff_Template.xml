              <ExprLine>
                <DescStr></DescStr>
                <OrigStr>
					DeclareType("GenInstanceData", "UIGenEntityBuff");
					if (GenInstanceData.Duration &lt;= 0 or GenInstanceData.Duration &gt;= 3600)
						1
					else
						# New icons should start full
						if (Self.Var[DisplayTime].Float = -1)
							GenSetValue(Self, "TargetTime", GenInstanceData.Duration);
							GenSetValue(Self, "DisplayTime", GenInstanceData.Duration);
						endif

						# If the time has reset, start over (Determine reset by a more than 10% jump up)
						if ((GenInstanceData.Duration / Self.Var[DisplayTime].Float) &gt;= 1.1)
							GenSetValue(Self, "DisplayTime", GenInstanceData.Duration);
							GenSetValue(Self, "TargetTime", GenInstanceData.Duration);
						endif

						# If the duration is ever larger than the largest value we've seen, remember it as the max value
						if (Self.Var[GreatestSeenTime].Float &lt; GenInstanceData.Duration)
							GenSetValue(Self, "GreatestSeenTime", GenInstanceData.Duration);
						endif

						# Set the target display time. If not updated from the server, then tick based on elapsed time to reduce choppiness.
						if (not Self.Var[LastSeenTime].int = GenInstanceData.Duration)
							GenSetValue(Self, "TargetTime", GenInstanceData.Duration);
						else
							GenSetValue(Self, "TargetTime", Self.Var[TargetTime].Float - ElapsedTime);
						endif

						# Tick down the elapsed time
						GenSetValue(Self, "DisplayTime", Self.Var[DisplayTime].Float - .1*(Self.Var[DisplayTime].Float - Self.Var[TargetTime].Float));

						GenSetValue(Self, "LastSeenTime", GenInstanceData.Duration);

						if (Self.Var[GreatestSeenTime].Float &gt; 0)
							Self.Var[DisplayTime].Float/Self.Var[GreatestSeenTime].Float
						endif
					endif
				</OrigStr>
