            <OrigStr>
			# Don't move back if at Jan 1st, 2010
			if (Gen("Calendar_Root").Var[Day].Int = 1 and Gen("Calendar_Root").Var[Month].Int = 1 and Gen("Calendar_Root").Var[Year].Int = 2010 )
				# Nothing
			else
				GenSetValue(Gen("Calendar_Root"), "Day", Gen("Calendar_Root").Var[Day].Int - 1);
				if (Gen("Calendar_Root").Var[Day].Int = 0)
					GenSetValue(Gen("Calendar_Root"), "Day", DaysInMonth(Gen("Calendar_Root").Var[Month].Int, Gen("Calendar_Root").Var[Year].Int));
					GenSetValue(Gen("Calendar_Root"), "Month", Gen("Calendar_Root").Var[Month].Int - 1);
					if (Gen("Calendar_Root").Var[Month].Int = -1)
						GenSetValue(Gen("Calendar_Root"), "Month", 11);
