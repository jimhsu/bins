      <Statement>
        <ExprLine>
          <DescStr></DescStr>
          <OrigStr>
			DeclareType("RowData", "PowerTreeStep");
			if (GetStepType(RowData) = "Enhancement")
				GenSetString(Self, "Name", GetStepPowerName(RowData) + ": ");
			else
				GenSetString(Self, "Name", "");
			Endif
			GenAppendString(Self, "Name", GetStepName(RowData));
			GenSetValue(Self, "Cost", RowData.CostRespec);
			if (GetStepType(RowData) = "PTNode" and RowData.Rank &gt;= 0)
				GenSetString(Self, "Rank", IntToString(RowData.Rank + 1));
				if (RowData.Rank)
					GenSetString(Self, "RankText", Translate("RetCon_ListRow_RankText"));
				else
					GenSetString(Self, "RankText", "");
				endif
			else
				GenSetString(Self, "RankText", "");
			endif
			GenSetString(Self, "Desc", GetStepDesc(RowData));

			if (Self.State.Row = 0)
				GenSetValue(Gen("RetCon_CurrentPower_UndoButton"), "Steps", 1 + RowData.StepsImplied);
				if (RowData.CostRespec &gt; InventoryGetNumericValue(Player, "Resources"))
					GenSetState(Gen("Retcon_CurrentPower_UndoButton"), Disabled, 1)
				else
					GenSetState(Gen("Retcon_CurrentPower_UndoButton"), Disabled, 0)
				endif
			else
				GenSetState(Self, Disabled, 1);
			endif
		</OrigStr>
        </ExprLine>
      </Statement>
