						GenSetString(Gen("Contact_ConfirmResponseDialog"), "BodyText", RowData.ConfirmText);
					elif (RowData.ShowRewardChooser)
						GenAddModal( Gen("Contact_RewardChooser"));
						GenSetString(Gen("Contact_RewardChooser"), "ResponseKey", RowData.Key);
						GenSendMessage(Gen("Contact_ListScreen_ScrollView"), "ScrollbarReset");
					else
						ContactDialogRespond(Player, RowData.Key, "");
						GenSendMessage(Gen("Contact_ListScreen_ScrollView"), "ScrollbarReset");
					endif
